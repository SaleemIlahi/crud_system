<%- include('./include/header.ejs')%>
    <div class="container">
        <h1 class="text-center text-color-update head py-4">Update Student Details</h1>
        <div class="row">
            <div class="col-5">
                <img src="images/pic-2.jpg" alt="studet-pic" />
            </div>
            <div class="col-7">
                <form method="post" class="p-5">
                    <div class="form-group my-4">
                        <label for="studentName" class="form-label">Update Student Name</label>
                        <input type="text" id="studentName" class="form-control" placeholder="Enter Your name" />
                    </div>
                    <div class="form-group my-4">
                        <label for="studentCourse" class="form-label">Update Course</label>
                        <input type="text" id="studentCourse" class="form-control" placeholder="Enter Your Course" />
                    </div>
                    <div class="form-group my-4">
                        <label for="studentPercent" class="form-label">Update Pervious Year Percentage</label>
                        <input type="text" id="studentPercent" class="form-control"
                            placeholder="Enter Your Precentage" />
                    </div>
                    <div class="button-group mt-5 d-flex justify-content-evenly">
                        <button type="submit" class="btn btn-primary">Update</button>
                        <a href="/student/view"
                            class="btn btn-primary d-flex flex-row align-items-center justify-content-center">View
                            Student</a>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <%- include('./include/footer.ejs')%>